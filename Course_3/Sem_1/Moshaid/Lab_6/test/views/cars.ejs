
<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link rel="stylesheet" href="/styles.css">
  <title>Cars Page</title>
</head>
<body>
  <h1>Cars Page</h1>
  <table>
    <thead>
      <tr>
        <th>Make</th>
        <th>Model</th>
        <th>Year</th>
        <th>Price</th>
      </tr>
    </thead>
    <tbody>
      <% cars.forEach(car => { %>
        <tr>
          <td><%- car.make %></td>
          <td><%- car.model %></td>
          <td><%- car.year %></td>
          <td>$<%- car.price %></td>
        </tr>
      <% }); %>
    </tbody>
  </table>
</body>
</html>
